{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "assfonts",
    "version": "0.6.3",
    "dependencies": [
        {
            "name": "harfbuzz",
            "features": [
                "experimental-api"
            ]
        },
        "freetype",
        "nlohmann-json",
        "libiconv",
        "fmt",
        "cli11",
        "ghc-filesystem",
        "pcre2",
        "string-view-lite",
        {
            "name": "qtbase",
            "default-features": false,
            "features": [
                "concurrent",
                "dbus",
                "default-features",
                "doubleconversion",
                "freetype",
                "gui",
                "harfbuzz",
                "icu",
                "jpeg",
                {
                    "name": "opengl",
                    "platform": "!ios"
                },
                "pcre2",
                "png",
                "thread",
                "widgets",
                "zstd"
            ]
        },
        {
            "name": "cpp-httplib",
            "features": [
                "openssl"
            ]
        },
        {
            "name": "ecm",
            "platform": "linux"
        }
    ]
}